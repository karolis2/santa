@model IEnumerable<Darbuotojai.Application.ViewModels.DarbuotojasDto>
@{
    ViewData["Title"] = "Darbuotojai CRUD";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Vardas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Pavardė)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AsmensKodas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.GimimoData)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NamoNumeris)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Gatve)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Miestas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PastoKodas)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Aktyvus)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Vardas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Pavardė)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AsmensKodas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.GimimoData)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NamoNumeris)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Gatve)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Miestas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PastoKodas)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Aktyvus)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
